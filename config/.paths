pathadd $HOME/.local/bin
pathadd $HOME/bin
pathadd $DOTFILES_ROOT/bin
pathadd /usr/local/man MANPATH

# OMZ
pathadd $HOME/.oh-my-zsh ZSH

# NVM
pathadd $HOME/.nvm NVM_DIR

# pyenv
pathadd $HOME/.pyenv PYENV_ROOT
pathadd $PYENV_ROOT/bin

# Go path (for KinD)
pathadd $HOME/go/bin

# # For Homebrew (likes to be before bin)
prepend_path /home/linuxbrew/.linuxbrew/bin
pathadd /home/linuxbrew/.linuxbrew/share/man MANPATH